<section id="review">
  <%= form_tag "/items/#{@item.id}/reviews" do %>
    <%= label_tag :title %>
    <%= text_field_tag :title %>

    <%= label_tag :content %>
    <%= text_field_tag :content %>

    <%= label_tag :rating %>
    <%= text_field_tag :rating %>

    <%= submit_tag "Submit" %>
  <% end %>
</section>
